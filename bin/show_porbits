# Functions 
goodfunc(){

for f in `find . -name slave.res`; do
   grep -H "^precise_orbits:" ${f}
done 
}


case $# in
         0) goodfunc 
	    ;;
         *) echo -e "Usage: ${0##*/} (run within INSAR processing directory) \n"
	    ;;
esac



